datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  Description   String?
  Name          String
  StockQuantity Int
  createdAt     DateTime @default(now())
  id            String   @id @default(cuid())
  price         Decimal
  updatedAt     DateTime @updatedAt
}
